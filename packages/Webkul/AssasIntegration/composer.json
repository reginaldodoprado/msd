{
    "name": "ds/assas_integration",
    "description": "Pacote Laravel para integração com API de pagamentos Asaas",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "DS",
            "email": "contato@ds.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Ds\\AssasIntegration\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ds\\AssasIntegration\\Providers\\AssasIntegrationServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
